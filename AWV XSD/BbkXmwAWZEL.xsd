		<xs:annotation>
			<xs:documentation> max. 18 Stellen Betrag mit optionalem Vorzeichen links von der ersten Ziffer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\s{0,17}-{0,1}\d{1,18}"/>
		</xs:restriction>
	</xs:simpleType>
