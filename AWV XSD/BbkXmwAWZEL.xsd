	<xs:include schemaLocation="BbkXmwBasis.xsd"/>
	<!-- ******                Elemente der Substitutionsgruppe IDENTNR   ******* -->
	<xs:element name="BLZ" type="bbk:zif8plus1" substitutionGroup="bbk:IDENTNR">
		<xs:annotation>
			<xs:documentation>Bankleitzahl (mit Prüfziffer)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TESTLZ" substitutionGroup="bbk:IDENTNR">
		<xs:annotation>
			<xs:documentation>Test-Leitzahl (ohne Prüfziffer)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="bbk:alphanum">
				<xs:pattern value="T\d\d\d\d\d\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="RZLZ" substitutionGroup="bbk:IDENTNR">
		<xs:annotation>
			<xs:documentation>Leitzahl eines Rechenzentrums (ohne Prüfziffer)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="bbk:alphanum">
				<xs:pattern value="R\d\d\d\d\d\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="FIRMENNR" type="bbk:zif7plus1" substitutionGroup="bbk:IDENTNR">
		<xs:annotation>
			<xs:documentation>Firmennummer (mit  Prüfziffer)</xs:documentation>
		</xs:annotation>
	</xs:element>
