	<xs:simpleType name="TerminalbereitstellungTyp">
		<xs:annotation>
			<xs:documentation>Terminalbereitstellungs-Typen</xs:documentation>
		</xs:annotation>
		<xs:restriction base="bbk:alphanum">
			<xs:enumeration value="A"/>
