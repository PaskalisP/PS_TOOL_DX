	<xs:simpleType name="PosZvs5Typ">
		<xs:annotation>
			<xs:documentation>Zvs5-Positionen</xs:documentation>
		</xs:annotation>
		<xs:restriction base="bbk:alphanum">
			<xs:enumeration value="T0.S1"/>
