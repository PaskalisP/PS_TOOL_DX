            <xs:element name="PrtryTxId" type="Max105Text">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">ProprietaryTransactionIdentification</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Internal unique transaction identifier used by the reporting agent for each transaction. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="CtrPtyPrtryTxId" type="Max105Text">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">CounterpartyProprietaryTransactionIdentification</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Internal unique proprietary transaction identifier as assigned by the counterparty of the reporting agent for each transaction.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CtrPtyId" type="CounterpartyIdentification1Choice">
